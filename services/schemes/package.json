{
  "name": "schemes-service",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "install-all": "npm --prefix backend install && npm --prefix frontend install",
    "start-backend": "npm --prefix backend start",
    "start-frontend": "npm --prefix frontend run dev -- --port=5300",
    "start-all": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
